{
    "name": "soup/cms",
    "description": "CMS for mobile and web applications",
    "authors": [
        {
            "name": "Daniel Welsh",
            "email": "x@x.x"
        }
    ],
    "require": {
        "php": ">=5.2.0",
        "illuminate/support": "5.2.*",
        "doctrine/dbal": "2.4.*",
        "laravelcollective/html": "5.2.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/controllers",
            "src/models",
            "src/views",
            "src/lib"
        ],
        "psr-4": {
            "Soup\\CMS\\": "src/",
            "Collective\\Html\\": "vendor/laravelcollective/html/src/"
        }
    },
    "minimum-stability": "stable"
}
